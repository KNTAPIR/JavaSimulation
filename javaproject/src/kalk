import java.util.Scanner;  // Importujemy Scanner do wczytywania danych od uÅ¼ytkownika

class Kalkulator {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);  // Tworzymy obiekt Scanner
        double[] liczby = new double[20];
        char[] dzialania = new char[20];
        int i = 0;
        boolean kk = false;
        for (i = 0; i<20 ;i=i+2) {
            // Wprowadzenie pierwszej liczby
            System.out.print("Podaj  liczbÄ™: ");
            liczby[i] = scanner.nextDouble();


            // WybÃ³r dziaÅ‚ania
            System.out.print("Wybierz dziaÅ‚anie (+, -, *, /): ");
            dzialania[i] = scanner.next().charAt(0);

            System.out.print("Podaj  liczbÄ™: ");
            liczby[i+1] = scanner.nextDouble();

            System.out.print("koniec? t/n");
            char odp = scanner.next().charAt(0);

            switch(odp){
                case 'n':
                    continue;

                case 't':
                    kk = true;
            }
            if(kk == true){
                break;
            }
        }
        // Wynik
        double wynik=0;
        double wynik_wlas=0;



        for(int j =0 ; j<=i; j++){
            // ObsÅ‚uga dziaÅ‚aÅ„
            switch (dzialania[j]) {
                case '+':
                    wynik = liczby[j] + liczby[j+1];
                    liczby[j] =wynik;
                    wynik_wlas = liczby[j];
                    break;
                case '-':
                    wynik = liczby[j] - liczby[j+1];
                    liczby[j] =wynik;
                    wynik_wlas = liczby[j];
                    break;
                case '*':
                    wynik = liczby[j] * liczby[j+1];
                    liczby[j] =wynik;
                    wynik_wlas = liczby[j];
                    break;
                case '/':
                    if (liczby[j+1] != 0) {
                        wynik = liczby[j] / liczby[j+1];
                        liczby[j] =wynik;
                        wynik_wlas = liczby[j];
                    } else {
                        System.out.println("âŒ BÅ‚Ä…d: Nie moÅ¼na dzieliÄ‡ przez zero!");
                        return;  // ZakoÅ„cz program, jeÅ›li dzielenie przez zero
                    }
                    break;
                default:
                    System.out.println("âŒ BÅ‚Ä…d: Nieznany operator.");
                    return;  // ZakoÅ„cz program, jeÅ›li operator jest nieprawidÅ‚owy
            }
        }
        // WyÅ›wietlenie wyniku
        System.out.println("Wynik: " + wynik_wlas);

        // ZamkniÄ™cie Scannera
        scanner.close();
    }
}





// Klasa implementujÄ…ca interfejs Runnable (lepsza praktyka niÅ¼ rozszerzanie Thread)
class Licznik implements Runnable {
    public void run() {
        for (int i = 1; i <= 10; i++) {
            System.out.println("Licznik: " + i);
            try {
                Thread.sleep(1000);  // OpÃ³Åºnienie 1 sekunda
            } catch (InterruptedException e) {
                System.out.println("WÄ…tek licznika zostaÅ‚ przerwany.");
            }
        }
    }
}

// Drugi wÄ…tek
class Komunikat implements Runnable {
    public void run() {
        while (true) {
            System.out.println("ðŸš¨ Przypomnienie: PamiÄ™taj o przerwie!");
            try {
                Thread.sleep(3000);  // Komunikat co 3 sekundy
            } catch (InterruptedException e) {
                System.out.println("WÄ…tek komunikatu zostaÅ‚ przerwany.");
                break;  // Zatrzymanie pÄ™tli przy przerwaniu
            }
        }
    }
}

// Klasa gÅ‚Ã³wna uruchamiajÄ…ca oba wÄ…tki
class WielowatkowoscPrzyklad {
    public static void main(String[] args) {
        // Tworzenie obiektÃ³w wÄ…tkÃ³w
        Thread watek1 = new Thread(new Licznik());
        Thread watek2 = new Thread(new Komunikat());

        // Uruchomienie wÄ…tkÃ³w
        watek1.start();
        watek2.start();

        // Czekanie na zakoÅ„czenie wÄ…tku 1, potem przerwanie wÄ…tku 2
        try {
            watek1.join();  // Czeka na zakoÅ„czenie wÄ…tku 1
            watek2.interrupt();  // Przerywa wÄ…tek 2
        } catch (InterruptedException e) {
            System.out.println("GÅ‚Ã³wny wÄ…tek zostaÅ‚ przerwany.");
        }

        System.out.println("âœ… Program zakoÅ„czony.");
    }
}












import javax.swing.*;
import java.awt.*;
import java.awt.event.*;

class KulkaPanel extends JPanel {
    private int x1 = 50, y1 = 50;   // Pozycja 1. kulki
    private int x2 = 200, y2 = 200; // Pozycja 2. kulki
    private int dx1 = 2, dy1 = 2;   // Kierunek ruchu 1. kulki
    private int dx2 = -3, dy2 = 3;  // Kierunek ruchu 2. kulki

   public KulkaPanel() {
        setPreferredSize(new Dimension(400, 400)); // Rozmiar okna

        // WÄ…tek dla 1. kulki
        new Thread(() -> {
            while (true) {
                x1 += dx1;
                y1 += dy1;
                if (x1 <= 0 || x1 >= getWidth() - 30) dx1 = -dx1;  // Odbicie od Å›ciany
                if (y1 <= 0 || y1 >= getHeight() - 30) dy1 = -dy1;
                repaint(); // OdÅ›wieÅ¼enie rysunku
                sleep(10);
            }
        }).start();

        // WÄ…tek dla 2. kulki
        new Thread(() -> {
            while (true) {
                x2 += dx2;
                y2 += dy2;
                if (x2 <= 0 || x2 >= getWidth() - 30) dx2 = -dx2;
                if (y2 <= 0 || y2 >= getHeight() - 30) dy2 = -dy2;
                repaint();
                sleep(15);
            }
        }).start();

    }

    // Metoda rysujÄ…ca kulki
    protected void paintComponent(Graphics g) {
        super.paintComponent(g);
        g.setColor(Color.RED);   // Kolor 1. kulki
        g.fillOval(x1, y1, 30, 30);

        g.setColor(Color.BLUE);  // Kolor 2. kulki
        g.fillOval(x2, y2, 30, 30);
    }

    // Metoda pomocnicza do spowolnienia ruchu kulek
    private void sleep(int ms) {
        try {
            Thread.sleep(ms);
        } catch (InterruptedException e) {
            Thread.currentThread().interrupt();
        }
    }
}

